<button type="button" class="btn btn-primary btn-lg btn-block sm-taleft"  data-toggle="collapse" data-target="#account-details">
	<b class="acc-name"></b> | <span class="acc-intitution-name"></span>
</button>
<div id="account-details" class="collapse">
	<div class="xs-spacer15 sm-spacer30"></div>
	<div class="row">
		<div class="col-md-12">
			<h6>{c2r-acc-description}</h6>
			<p class="acc-desc"></p>
		</div>
	</div>
	<div class="row">
		<div class="col-md-4">
			<h6>{c2r-balance}</h6>
			<h3 class="acc-balance"></h3>
		</div>
		<div class="col-md-4">
			<h6>{c2r-balance-estimate}</h6>
			<h3 class="acc-balance-estimate"></h3>
		</div>
		<div class="col-md-4">
			<h6>{c2r-balance-invoices-not-paied}</h6>
			<h3 class="acc-invoices-not-paied"></h3>
		</div>
	</div>
	<hr>
	<div class="row">
		<div class="col-md-8">
			<h6>{c2r-acc-institutionName}</h6>
			<p class="acc-intitution-name"></p>
		</div>
		<div class="col-md-4">
			<h6>{c2r-acc-number}</h6>
			<p class="acc-number"></p>
		</div>
	</div>
	<hr>
	<div class="row">
		<div class="col-md-8">
			<h6>{c2r-acc-iban}</h6>
			<p class="acc-iban"></p>
		</div>
		<div class="col-md-4">
			<h6>{c2r-acc-swift}</h6>
			<p class="acc-swift"></p>
		</div>
	</div>
	<hr>
	<div class="row">
		<div class="col-md-12">
			<h6>{c2r-acc-code}</h6>
			<p class="acc-code"></p>
		</div>
	</div>
</div>
